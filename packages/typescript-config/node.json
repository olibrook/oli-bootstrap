{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "target": "esnext",
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "Bundler",
    "noEmit": true,
    "skipLibCheck": true,

    "outDir": "dist"
  }
}
